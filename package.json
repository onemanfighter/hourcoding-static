{
  "name": "hourcoding-static",
  "version": "0.0.1",
  "description": "Static server to host static assets resource",
  "main": "node_src/server.ts",
  "scripts": {
    "build": "tsc --project ./",
    "start:dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./node_src/server.ts",
    "start:prod": "node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1",
    "serve-index": "^1.9.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "@types/serve-index": "^1.9.4",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "repository": "git@github.com:onemanfighter/hourcoding-static.git",
  "author": "Amit Raikwar <managerarbusiness@gmail.com>",
  "license": "MIT"
}
